#!/bin/bash

usage()
{
   echo ""
   echo "Usage: $0 /some/directory/path"
   echo -e "This script downloads 101 images of U.S. city flag from Wikimedia"
   echo -e "to a directory on your machine. It relies on:"
   echo -e "\t1. python-2.7 environment."   
   echo -e "\t2. download_from_wikimedia_commons being installed"
   echo -e "Parameters:"
   echo -e "\tdirectory to store the images."
   exit 1
}


if [ -z "$1" ]
then
   usage
fi

download_from_wikimedia_commons Municipal_Flag_of_Chicago.svg --output $output_dir --width 480
